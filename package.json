    "css-loader": "^5.2.6",
    "cssnano": "^5.0.5",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-webpack-plugin": "^2.5.4",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.1",
    "postcss-loader": "^5.3.0",
    "postcss-safe-parser": "^5.0.2",
    "resolve-url-loader": "^4.0.0",
